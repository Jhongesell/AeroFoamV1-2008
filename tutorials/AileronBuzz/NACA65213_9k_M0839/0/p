FoamFile
{
    version 2.0;
    format ascii;

    root "/home/elisa/TESI/MESH_GENERATION";
    case "NACA65213";
    instance "0";
    local "";

    class volScalarField;
    object p;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
    inlet-outlet
    {
        type            fixedValue;
        value           uniform 101325;
    }
    body
    {
        type            slip;
    }
    defaultFaces
    {
        type            empty;
    }
}


